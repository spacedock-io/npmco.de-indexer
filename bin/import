#!/usr/bin/env node
var import_ = require('../lib/import');

var package = process.argv[2].split('@'),
    name = package[0],
    version = package[1];

var importer = import_({
  name: name,
  version: version,
  elasticsearch: {
    host: 'localhost',
    port: 9200
  }
});
importer.on('done', function () {
  console.dir(arguments);
});
